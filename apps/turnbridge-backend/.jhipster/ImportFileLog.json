{
  "annotations": {
    "changelogDate": "20251116033152"
  },
  "applications": "*",
  "documentation": "匯入批次事件紀錄（非逐行）",
  "dto": "mapstruct",
  "fields": [
    {
      "documentation": "事件代碼（如 PARSE_ERROR/NORMALIZE_SUMMARY）",
      "fieldName": "eventCode",
      "fieldType": "String",
      "fieldValidateRules": ["required", "maxlength"],
      "fieldValidateRulesMaxlength": "64"
    },
    {
      "documentation": "層級（INFO/WARN/ERROR）",
      "fieldName": "level",
      "fieldType": "String",
      "fieldValidateRules": ["required", "maxlength"],
      "fieldValidateRulesMaxlength": "16"
    },
    {
      "documentation": "訊息摘要",
      "fieldName": "message",
      "fieldType": "String",
      "fieldValidateRules": ["maxlength"],
      "fieldValidateRulesMaxlength": "1024"
    },
    {
      "documentation": "詳細內容（JSON 或堆疊）",
      "fieldName": "detail",
      "fieldType": "TextBlob"
    },
    {
      "documentation": "發生時間",
      "fieldName": "occurredAt",
      "fieldType": "Instant"
    }
  ],
  "jpaMetamodelFiltering": true,
  "name": "ImportFileLog",
  "pagination": "pagination",
  "relationships": [
    {
      "documentation": "匯入檔主檔",
      "otherEntityField": "originalFilename",
      "otherEntityName": "importFile",
      "relationshipName": "importFile",
      "relationshipSide": "left",
      "relationshipType": "many-to-one",
      "relationshipValidateRules": "required"
    }
  ],
  "searchEngine": "no",
  "service": "serviceClass"
}
